{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "ordered-imports": [false],
    "semicolon": [true, "never"],
    "object-literal-sort-keys": [false],
    "no-console": [false],
    "interface-name": [true, "never-prefix"],
    "quotemark": [false],
    "no-bitwise": false,
    "jsx-no-lambda": false,
    "jsx-no-bind": false
  },
  "rulesDirectory": []
}
